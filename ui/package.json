{
  "name": "agent-director-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:e2e": "playwright test",
    "test:e2e:review": "playwright test --config playwright.review.config.ts",
    "lhci": "bash ./scripts/lhci.sh",
    "percy:playwright": "percy exec -- playwright test tests/e2e/visual.spec.ts tests/e2e/ux-review.spec.ts"
  },
  "dependencies": {
    "dagre": "^0.8.5",
    "framer-motion": "^11.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@eslint/js": "^9.5.0",
    "@lhci/cli": "^0.13.0",
    "@percy/cli": "^1.31.1",
    "@percy/playwright": "^1.0.7",
    "@playwright/test": "^1.48.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/dagre": "^0.7.52",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.5.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  }
